<h1>New post:</h1>
<%= form_for @post do |f|%>

    <%= f.label :title%>
    <%= f.text_field :title%><br>

    <%= f.label :content%>
    <%= f.text_field :content%><br>

    <%= f.collection_select :blogger_id, Blogger.all, :id, :name%><br>
    <%= f.collection_select :destination_id, Destination.all, :id, :name%><br>

    <%= f.submit%>
<% end %>